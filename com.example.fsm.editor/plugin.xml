<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated fsm -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="com.example.fsm.presentation.FSMModelWizardID"
            name="%_UI_FSMModelWizard_label"
            class="com.example.fsm.presentation.FSMModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/FSMModelFile.gif">
         <description>%_UI_FSMModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated fsm -->
      <editor
            id="com.example.fsm.presentation.FSMEditorID"
            name="%_UI_FSMEditor_label"
            icon="icons/full/obj16/FSMModelFile.gif"
            extensions="fsm"
            class="com.example.fsm.presentation.FSMEditor"
            contributorClass="com.example.fsm.presentation.FSMActionBarContributor">
      </editor>
   </extension>

</plugin>
